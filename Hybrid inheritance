class A:
    def display(self):
        print("display from A class")
class B(A):
    def display(self):
        print("display from B class")
class C:
    def show(self):
        print("Hii from class c")
class D(B,C):
    def display(self):
        print("display from D class")
d1=D()
d1.display()
print(D.mro())
