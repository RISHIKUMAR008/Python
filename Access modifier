class Student:
    def __init__(self, Name, rollno):
        self._rollno = rollno
        self.name = Name

    def display(self):
        print(f"Hi, myself {self.name} from the Student class.")

class Branch(Student):
    pass

b1 = Branch("Nisha", 22)  

print(b1.name)
print(b1._rollno)

s1 = Student("Rahul", 25) 
print(s1.name)
s1.display()
